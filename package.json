{
  "name": "shop-buddies-core",
  "version": "1.0.0",
  "type": "module",
  "module": "esnext",
  "scripts": {
    "dev": "gulp watching",
    "build": "gulp",
    "tw": "gulp tBuild",
    "prettify": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" --ignore-unknown",
    "theme-check": "shopify theme check",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
    "cli": "shopify theme dev -e default",
    "cli:sync": "shopify theme dev -e default --theme-editor-sync",
    "create:section": "node ./devops/dist/CLI/section-creation.js",
    "create:snippet": "node ./devops/dist/CLI/snippet-creation.js",
    "build:devops": "tsc -p tsconfig.devops.json"
  },
  "private": true,
  "devDependencies": {
    "@types/gulp": "^4.0.17",
    "@types/gulp-postcss": "^8.0.6",
    "@types/gulp-rename": "^2.0.6",
    "@types/gulp-sass": "^5.0.4",
    "@types/gulp-terser": "^1.2.6",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/through2": "^2.0.41",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "gulp": "^5.0.0",
    "gulp-ejs": "^5.1.0",
    "gulp-esbuild": "^0.14.0",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^6.0.0",
    "gulp-terser": "^2.1.0",
    "postcss-prefix-selector": "^2.1.0",
    "prettier": "^3.4.2",
    "sass": "^1.83.1",
    "tailwindcss": "^3.4.17",
    "through2": "^4.0.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "swiper": "^11.2.0",
    "zod": "^3.24.1"
  }
}
